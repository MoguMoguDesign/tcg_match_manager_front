# Firebase プロジェクト設定
FIREBASE_PROJECT_ID=your-project-id

# サービスアカウントキーのパス
SERVICE_ACCOUNT_KEY_PATH=./serviceAccountKey.json

# エミュレータ使用（true/false）
USE_EMULATOR=false

# エミュレータホスト（USE_EMULATOR=true の場合のみ）
EMULATOR_HOST=localhost:8080

# 投入するデータセット（カンマ区切り）
# 利用可能: small, bye, completed, preparing, all
DATASETS=all
